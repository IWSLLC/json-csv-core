{
  "name": "@iwsio/json-csv-core",
  "version": "0.2.3",
  "description": "A core library for browser and node to support and extend json-csv project's original goal.",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:js": "mocha $@",
    "test:ts": "mocha --config .mocharc-ts.json $@",
    "prebuild": "rimraf dist",
    "build:js": "babel src --out-dir dist --source-maps",
    "build:ts": "tsc",
    "build": "npm-run-all build:js build:ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IWSLLC/json-csv-core.git"
  },
  "keywords": [
    "json",
    "csv",
    "node",
    "browser"
  ],
  "browserslist": [
    "defaults"
  ],
  "author": "Nathan Bridgewater",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/IWSLLC/json-csv-core/issues"
  },
  "homepage": "https://github.com/IWSLLC/json-csv-core#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2"
  }
}
